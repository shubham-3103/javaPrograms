<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="ls" class="com.edu.spring.ConstructorListExample">
		<constructor-arg value="Shubham Sharma"></constructor-arg>
		<constructor-arg>
			<list>
				<value>Shubham</value>
				<value>Siddharth</value>
				<value>Garv</value>
			</list>
		</constructor-arg>
	</bean>
	<bean id="mp" class="com.edu.spring.ConstructorMapExmp">
		<constructor-arg value="Shubham Sharma"></constructor-arg>
		<constructor-arg>
			<map>
				<entry key="1" value="Shubham"></entry>
				<entry key="2" value="Siddharth"></entry>
			</map>
		</constructor-arg>
	</bean>
</beans>