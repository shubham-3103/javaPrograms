<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="e" class="com.edu.spring.Customer">
		<constructor-arg value="10" type="int"></constructor-arg>
	</bean>
	<bean id="first" class="com.edu.spring.Ant">
		<constructor-arg value="Shubham"></constructor-arg>
		<constructor-arg value="Sharma"></constructor-arg>
	</bean>
	<bean id="second" class="com.edu.spring.SpringConstructorExmp">
		<constructor-arg value="5" type="int"></constructor-arg>
		<constructor-arg value="6" type="int"></constructor-arg>
	</bean>
	<bean id="constructor" class="com.hughes.setter.Customer">
		<property name="customerId" value="500"></property>
		<property name="custName" value="Shubham"></property>
	</bean>
</beans>